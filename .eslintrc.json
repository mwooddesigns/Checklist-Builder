{
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "linebreak-style": [
            2,
            "windows"
        ],
        "quotes": [
            2,
            "double"
        ],
        "semi": [
            1,
            "always"
        ],
        "no-unused-vars": [
          1
        ],
        "no-undef": [
          1
        ],
        "no-console": [
          1
        ]
    }
}
